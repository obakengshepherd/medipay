generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Payment {
  id        Int      @id @default(autoincrement())
  amount    Float
  recipient String
  createdAt DateTime @default(now())
}